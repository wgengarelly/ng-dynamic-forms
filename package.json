{
  "name": "ng-dynamic-forms",
  "version": "7.1.0",
  "description": "A rapid form development library for Angular",
  "scripts": {
    "aot-compile:sample": "ngc -p sample/tsconfig.aot.ngc.json",
    "bundle:sample:rollup": "rollup -c sample/rollup.ngc.config.js",
    "bundle:sample:webpack": "webpack --config sample/webpack.jit.config.js",
    "build:sample": "webpack --config sample/webpack.aot.ngtools.config.js",
    "build:packages": "gulp build",
    "gulp": "gulp",
    "lint:packages": "tslint -c tslint.json './packages/**/*.ts'",
    "publish": "./publish.sh",
    "rollup": "rollup",
    "start": "lite-server -c bs-config.js",
    "server:jt": "webpack-dev-server --config sample/webpack.jit.config.js",
    "tests:e2e": "protractor protractor.conf.js",
    "tests:unit": "karma start karma.conf.js --browsers ChromeHeadless --single-run",
    "tests:unit:debug": "karma start karma.conf.js --browsers Chrome",
    "transpile:sample:es2015": "tsc -p sample/tsconfig.jit.json -m es2015",
    "transpile:sample:commonjs": "tsc -p sample/tsconfig.jit.json -m commonjs",
    "update": "./update.sh",
    "watch:sample": "tsc -p sample/tsconfig.jit.json -m commonjs -watch",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udos86/ng-dynamic-forms.git"
